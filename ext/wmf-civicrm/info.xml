<?xml version="1.0"?>
<extension key="wmf-civicrm" type="module">
  <file>wmf_civicrm</file>
  <name>WMF CiviCRM</name>
  <description>Generic wmf extension</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>eileen</author>
    <email>emcnaughton@wikimedia.org</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">http://FIXME</url>
    <url desc="Documentation">http://FIXME</url>
    <url desc="Support">http://FIXME</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2021-08-31</releaseDate>
  <version>1.1</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.79</ver>
  </compatibility>
  <comments>This is a intended as something of a catchall for wmf functionality.
    It is intended to replace the wmf_civicrm module over time
  </comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
    <psr0 prefix="CRM_" path="."/>
  </classloader>
  <requires>
    <ext>checksum_invalidator</ext>
    <ext>deduper</ext>
    <ext>org.wikimedia.omnimail</ext>
    <ext>au.org.greens.extendedmailingstats</ext>
    <ext>org.wikimedia.contacteditor</ext>
    <ext>org.civicrm.afform</ext>
    <ext>org.civicrm.afform_admin</ext>
    <ext>org.wikimedia.geocoder</ext>
    <ext>nz.co.fuzion.civitoken</ext>
    <ext>nz.co.fuzion.extendedreport</ext>
    <ext>org.wikimedia.smashpig</ext>
    <ext>org.wikimedia.wmffraud</ext>
    <ext>org.wikimedia.rip</ext>
    <ext>org.wikimedia.unsubscribeemail</ext>
    <ext>org.wikimedia.datachecks</ext>
    <ext>org.wikimedia.forgetme</ext>
    <ext>org.wikimedia.relationshipblock</ext>
    <ext>org.wikimedia.systemtools</ext>
    <ext>org.civicrm.contactlayout</ext>
    <ext>org.civicrm.recentmenu</ext>
    <ext>org.wikimedia.thethe</ext>
    <ext>uk.org.futurefirst.networks.emailamender</ext>
    <ext>wmf-thankyou</ext>
    <ext>ckeditor5</ext>
    <ext>assignee</ext>
    <ext>matching-gifts</ext>
    <ext>rpow</ext>
    <ext>monolog</ext>
    <ext>net.ourpowerbase.exportpermission</ext>
    <ext>message_admin</ext>
    <ext>exchange_rates</ext>
    <ext>civiimport</ext>
    <ext>import_extensions</ext>
    <ext>civicrm_admin_ui</ext>
    <ext>biz.lcdservices.movecontrib</ext>
    <ext>riverlea</ext>
    <ext>damaged</ext>
    <ext>symfony_email</ext>
    <ext>civi_campaign</ext>
  </requires>
  <civix>
    <namespace>CRM/Wmf</namespace>
    <format>25.10.2</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>mgd-php@2.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>entity-types-php@2.0.0</mixin>
    <mixin>smarty@1.0.3</mixin>
    <mixin>scan-classes@1.0.0</mixin>
  </mixins>
  <upgrader>CiviMix\Schema\WmfCivicrm\AutomaticUpgrader</upgrader>
</extension>
