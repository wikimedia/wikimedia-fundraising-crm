{
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/eileenmcnaughton/omnimail-silverpop.git"
    }
  ],
    "require": {
        "wikimedia/omnimail-silverpop" : "*",
        "league/csv" : "9.6.*",
        "guzzlehttp/guzzle": "^7.0",
        "omnimail/omnimail" : "*",
        "ext-dom": "*",
        "ext-libxml": "*"
    },
    "require-dev" : {
        "phpunit/phpunit": ">= 6.1"
    },
    "autoload": {
        "psr-4": {
          "Omnimail\\Silverpop\\": "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
          "Omnimail\\Silverpop\\Tests\\" : "test/tests"
        }
    }
}
