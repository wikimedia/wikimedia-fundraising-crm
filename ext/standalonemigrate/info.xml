<?xml version="1.0"?>
<extension key="standalonemigrate" type="module">
  <file>standalonemigrate</file>
  <name>Standalone Migrate</name>
  <description>Tools for migrating an existing CiviCRM site to CiviCRM Standalone</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Benjamin Walpole</author>
    <email>ben@civicrm.org</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://lab.civicrm.org/extensions/standalonemigrate</url>
    <url desc="Issues">https://lab.civicrm.org/extensions/standalonemigrate/-/issues</url>
    <url desc="Chat">https://chat.civicrm.org/civicrm/channels/standalone</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2025-04-16</releaseDate>
  <version>2.0-alpha</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.76</ver>
  </compatibility>
  <comments>This module is still being developed. Please raise issues on the chat or issue tracker above.</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Standalonemigrate</namespace>
    <format>23.02.1</format>
    <angularModule>crmStandalonemigrate</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
</extension>
