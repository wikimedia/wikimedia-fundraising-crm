<?xml version="1.0"?>
<extension key="org.wikimedia.unsubscribeemail" type="module">
  <file>unsubscribeemail</file>
  <name>Email unsubscribe form</name>
  <description>Adds a form for unsubscribing by email</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>eileen</author>
    <email>emcnaughton@wikimedia.org</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/eileenmcnaughton/org.wikimedia.unsubscribeemail</url>
    <url desc="Documentation">https://github.com/eileenmcnaughton/org.wikimedia.unsubscribeemail</url>
    <url desc="Support">https://github.com/eileenmcnaughton/org.wikimedia.unsubscribeemail</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-05-04</releaseDate>
  <version>1.4</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.70</ver>
  </compatibility>
  <comments>Unsubscribe emails, defined as setting is_opt_out = 1

      if primary and is_bulkmail = 0 on the email (for all)</comments>
  <civix>
    <namespace>CRM/Unsubscribeemail</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>ang-php@1.0.0</mixin>
  </mixins>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
</extension>
