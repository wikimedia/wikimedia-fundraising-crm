<?xml version="1.0"?>
<extension key="au.org.greens.extendedmailingstats" type="module">
  <file>extendedmailingstats</file>
  <name>Extended Mailing Statistics</name>
  <description>Provides more info on mailings</description>
  <license>The same as CiviCRM itself.  http://civicrm.org/what/licensing</license>
  <maintainer>
    <author>Andrew McNaughton</author>
    <email>andrew@mcnaughty.com</email>
  </maintainer>
  <releaseDate>2024-11-22</releaseDate>
  <version>1.1</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.79</ver>
  </compatibility>
  <comments>Developed for the Australian Greens, http://greens.org.au/</comments>
  <civix>
    <namespace>CRM/ExtendedMailingStats</namespace>
    <format>24.09.1</format>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>entity-types-php@2.0.0</mixin>
    <mixin>smarty@1.0.3</mixin>
  </mixins>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <upgrader>CiviMix\Schema\Extendedmailingstats\AutomaticUpgrader</upgrader>
</extension>
