<?xml version="1.0"?>
<extension key="deduper" type="module">
  <file>deduper</file>
  <name>Deduper</name>
  <description>Various tools to help with deduping</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>eileen</author>
    <email>emcnaughton@wikimedia.org</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/eileenmcnaughton/deduper</url>
    <url desc="Documentation">https://docs.civicrm.org/deduper/en/latest/</url>
    <url desc="Support">https://github.com/eileenmcnaughton/deduper/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-08-01</releaseDate>
  <version>1.10</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.75</ver>
  </compatibility>
  <comments>Tools to help with deduping</comments>
  <civix>
    <namespace>CRM/Deduper</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>afform-entity-php@1.0.0</mixin>
    <mixin>ang-php@1.0.0</mixin>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>entity-types-php@1.0.0</mixin>
    <mixin>smarty@1.0.3</mixin>
  </mixins>
  <classloader>
    <psr0 prefix="CRM_" path=""/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <upgrader>CRM_Deduper_Upgrader</upgrader>
</extension>
