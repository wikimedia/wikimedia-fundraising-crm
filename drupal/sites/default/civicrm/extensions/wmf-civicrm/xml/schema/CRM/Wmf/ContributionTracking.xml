<?xml version="1.0" encoding="iso-8859-1" ?>

<table>
  <base>CRM/Wmf</base>
  <class>ContributionTracking</class>
  <name>civicrm_contribution_tracking</name>
  <comment>CiviCRM Contribution Tracking table</comment>
  <add>5.61</add>
  <field>
    <name>id</name>
    <type>int unsigned</type>
    <length>10</length>
    <required>true</required>
    <comment>Unique Contribution Tracking ID</comment>
    <html>
      <type>Number</type>
    </html>
  </field>
  <primaryKey>
    <name>id</name>
  </primaryKey>
  <field>
    <name>contribution_id</name>
    <type>int unsigned</type>
    <length>10</length>
    <html>
      <type>Number</type>
    </html>
    <comment>FK to Contribution</comment>
  </field>
  <foreignKey>
    <name>contribution_id</name>
    <table>civicrm_contribution</table>
    <onDelete>SET NULL</onDelete>
    <key>id</key>
  </foreignKey>
  <field>
    <name>amount</name>
    <type>decimal</type>
    <length>20,3</length>
    <html>
      <type>Decimal</type>
    </html>
    <comment>Amount</comment>
  </field>
  <field>
    <name>currency</name>
    <type>varchar</type>
    <length>3</length>
    <html>
      <type>text</type>
    </html>
    <comment>Currency ISO Code</comment>
  </field>
  <index>
    <name>currency</name>
    <fieldName>currency</fieldName>
  </index>
  <field>
    <name>usd_amount</name>
    <type>decimal</type>
    <length>20,2</length>
    <html>
      <type>Decimal</type>
    </html>
    <comment>USD Amount</comment>
  </field>
  <field>
    <title>Is Recurring?</title>
    <name>is_recurring</name>
    <type>boolean</type>
    <html>
      <type>Boolean</type>
    </html>
  </field>
  <field>
    <name>referrer</name>
    <type>varchar</type>
    <length>4096</length>
    <html>
      <type>Text</type>
    </html>
    <comment>Referrer</comment>
  </field>
  <field>
    <name>utm_medium</name>
    <type>varchar</type>
    <length>128</length>
    <html>
      <type>Text</type>
    </html>
    <comment>UTM Medium</comment>
  </field>
  <index>
    <name>utm_medium_id</name>
    <fieldName>utm_medium</fieldName>
  </index>
  <field>
    <name>utm_campaign</name>
    <type>varchar</type>
    <length>128</length>
    <html>
      <type>Text</type>
    </html>
    <comment>UTM Campaign</comment>
  </field>
  <index>
    <name>utm_campaign_id</name>
    <fieldName>utm_campaign</fieldName>
  </index>
  <field>
    <name>utm_key</name>
    <type>varchar</type>
    <length>128</length>
    <html>
      <type>Text</type>
    </html>
    <comment>UTM Key</comment>
  </field>
  <field>
    <name>gateway</name>
    <type>varchar</type>
    <length>32</length>
    <html>
      <type>Text</type>
    </html>
    <comment>Gateway, e.g paypal_ec,adyen</comment>
  </field>
  <field>
    <name>appeal</name>
    <type>varchar</type>
    <length>64</length>
    <html>
      <type>Text</type>
    </html>
    <comment>e.g JimmyQuote - the appeal is the text to the left of the input boxes (on desktop-size screens)</comment>
  </field>
  <field>
    <name>payments_form_variant</name>
    <type>varchar</type>
    <length>64</length>
    <html>
      <type>Text</type>
    </html>
    <comment> a 'variant' generally changes something about the input boxes or their labels</comment>
  </field>
  <field>
    <name>banner</name>
    <type>varchar</type>
    <length>128</length>
    <html>
      <type>Text</type>
    </html>
    <comment>Banner</comment>
  </field>
  <index>
    <name>banner</name>
    <fieldName>banner</fieldName>
  </index>
  <field>
    <name>landing_page</name>
    <type>varchar</type>
    <length>128</length>
    <html>
      <type>Text</type>
    </html>
    <comment>Landing Page</comment>
  </field>
  <index>
    <name>landing_page</name>
    <fieldName>landing_page</fieldName>
  </index>
  <field>
    <name>payment_method_id</name>
    <title>Payment Method Family</title>
    <type>int</type>
    <pseudoconstant>
      <optionGroupName>payment_method</optionGroupName>
    </pseudoconstant>
    <html>
      <type>Select</type>
    </html>
    <readonly>true</readonly>
  </field>
  <field>
    <name>payment_submethod_id</name>
    <title>Specific Payment Method</title>
    <type>int</type>
    <pseudoconstant>
      <optionGroupName>payment_instrument</optionGroupName>
    </pseudoconstant>
    <html>
      <type>Select</type>
    </html>
    <readonly>true</readonly>
  </field>
  <index>
    <name>payment_method_id</name>
    <fieldName>payment_method_id</fieldName>
  </index>
  <field>
    <name>language</name>
    <type>varchar</type>
    <length>8</length>
    <html>
      <type>Text</type>
    </html>
    <comment>Language</comment>
  </field>
  <index>
    <name>language</name>
    <fieldName>language</fieldName>
  </index>
  <field>
    <name>country</name>
    <type>varchar</type>
    <length>2</length>
    <html>
      <type>Text</type>
    </html>
    <comment>Country</comment>
  </field>
  <index>
    <name>country</name>
    <fieldName>country</fieldName>
  </index>
  <field>
    <name>tracking_date</name>
    <type>datetime</type>
    <comment>Tracking Date</comment>
  </field>
  <index>
    <name>tracking_date</name>
    <fieldName>tracking_date</fieldName>
  </index>
  <field>
    <name>os</name>
    <type>varchar</type>
    <length>255</length>
    <comment>Data pending - see pending T182039. Operating System</comment>
  </field>
  <field>
    <name>os_version</name>
    <type>varchar</type>
    <length>255</length>
    <html>
      <type>Text</type>
    </html>
    <comment>Data pending - see pending T182039. Operating System - Major Version</comment>
  </field>
  <field>
    <name>browser</name>
    <type>varchar</type>
    <length>255</length>
    <html>
      <type>Text</type>
    </html>
    <comment>Data pending - see pending T182039. Browser</comment>
  </field>
  <field>
    <name>browser_version</name>
    <type>varchar</type>
    <length>255</length>
    <html>
      <type>Text</type>
    </html>
    <comment>Data pending - see pending T182039. Browser Version</comment>
  </field>
  <field>
    <name>recurring_choice_id</name>
    <title>Recurring Choice</title>
    <type>int</type>
    <comment>Denotes whether a recurring donation was the result of upsell or an organic recurring transaction</comment>
    <pseudoconstant>
      <optionGroupName>recurring_choice</optionGroupName>
    </pseudoconstant>
    <html>
      <type>Select</type>
    </html>
    <readonly>true</readonly>
  </field>
  <field>
    <name>device_type_id</name>
    <title>Device Type</title>
    <type>int</type>
    <comment>The device the banner was served to (e.g Desktop or Mobile)</comment>
    <pseudoconstant>
      <optionGroupName>device_type</optionGroupName>
    </pseudoconstant>
    <html>
      <type>Select</type>
    </html>
    <readonly>true</readonly>
  </field>
  <field>
    <name>banner_size_id</name>
    <title>Banner Size</title>
    <type>int</type>
    <comment>Large or small banner</comment>
    <pseudoconstant>
      <optionGroupName>banner_size</optionGroupName>
    </pseudoconstant>
    <html>
      <type>Select</type>
    </html>
    <readonly>true</readonly>
  </field>
  <field>
    <name>is_test_variant</name>
    <title>Is a test variant</title>
    <type>boolean</type>
    <comment>Test, rather than a control group</comment>
  </field>
  <field>
    <name>banner_variant</name>
    <title>Banner Variant</title>
    <type>varchar</type>
    <html>
      <type>Text</type>
    </html>
    <length>128</length>
    <comment>The name of the tested variant (if not control)</comment>
    <readonly>true</readonly>
  </field>
  <field>
    <name>is_pay_fee</name>
    <title>User opted to pay fee (PTF)</title>
    <type>boolean</type>
    <comment>Did the user select to pay the processing fee</comment>
  </field>
  <field>
    <name>mailing_identifier</name>
    <title>Mailing Identifier</title>
    <type>varchar</type>
    <length>32</length>
    <comment>External mailing identifier</comment>
  </field>
  <field>
    <name>utm_source</name>
    <title>UTM Source</title>
    <type>varchar</type>
    <length>128</length>
    <comment>UTM Source. This is the original text but is separately broken out into banner etc. We aspire to drop this field if we can be sure the data in it is clean enough to always capture in sub-fields</comment>
  </field>
  <index>
    <name>index_mailing_identifier</name>
    <fieldName>mailing_identifier</fieldName>
  </index>
</table>
