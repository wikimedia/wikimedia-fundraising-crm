<?xml version="1.0"?>
<extension key="wmf-civicrm" type="module">
  <file>wmf_civicrm</file>
  <name>WMF CiviCRM</name>
  <description>Generic wmf extension</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>eileen</author>
    <email>emcnaughton@wikimedia.org</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">http://FIXME</url>
    <url desc="Documentation">http://FIXME</url>
    <url desc="Support">http://FIXME</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2020-07-21</releaseDate>
  <version>1.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.27</ver>
  </compatibility>
  <comments>This is a intended as something of a catchall for wmf functionality. It is intended to replace the wmf_civicrm module over time</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <requires>
    <ext>deduper</ext>
    <ext>org.wikimedia.omnimail</ext>
    <ext>au.org.greens.extendedmailingstats</ext>
    <ext>org.wikimedia.contacteditor</ext>
    <ext>org.civicrm.afform</ext>
    <ext>deduper</ext>
    <ext>org.wikimedia.geocoder</ext>
    <ext>nz.co.fuzion.civitoken</ext>
    <ext>nz.co.fuzion.extendedreport</ext>
    <ext>org.wikimedia.smashpig</ext>
    <ext>org.wikimedia.wmffraud</ext>
    <ext>org.wikimedia.rip</ext>
    <ext>org.wikimedia.unsubscribeemail</ext>
    <ext>org.wikimedia.datachecks</ext>
    <ext>org.wikimedia.forgetme</ext>
    <ext>org.wikimedia.relationshipblock</ext>
    <ext>org.civicrm.shoreditch</ext>
    <ext>org.civicrm.angularprofiles</ext>
    <ext>org.civicrm.contactlayout</ext>
    <ext>org.wikimedia.thethe</ext>
    <ext>uk.org.futurefirst.networks.emailamender</ext>
    <ext>wmf-thankyou</ext>
    <ext>ckeditor5</ext>
    <ext>uk.squiffle.assignee</ext>
    <ext>matching-gifts</ext>
    <ext>civi-data-translate</ext>
    <ext>rpow</ext>
    <ext>monolog</ext>
  </requires>
  <civix>
    <namespace>CRM/Wmf</namespace>
  </civix>
</extension>
