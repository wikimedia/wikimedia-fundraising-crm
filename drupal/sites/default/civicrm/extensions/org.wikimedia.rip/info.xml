<?xml version="1.0"?>
<extension key="org.wikimedia.rip" type="module">
  <file>rip</file>
  <name>Civi-RIP</name>
  <description>Sets the "No Bulk E-mail (User opt out)" ('is_opt_out') flag on deceased people. On install existing contacts will be opted out.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>eileen</author>
    <email>emcnaughton@wikimedia.org</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/eileenmcnaughton/org.wikimedia.rip</url>
    <url desc="Documentation">https://github.com/eileenmcnaughton/org.wikimedia.rip</url>
    <url desc="Support">https://github.com/eileenmcnaughton/org.wikimedia.rip</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2023-11-03</releaseDate>
  <version>1.2</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.57</ver>
  </compatibility>
  <comments>CiviCRM functions usually respects the is_deceased flag so this is only necessary if it helps your data processes.</comments>
  <civix>
    <namespace>CRM/Rip</namespace>
    <format>23.02.1</format>
  </civix>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <upgrader>CRM_Rip_Upgrader</upgrader>
</extension>
